/* Write a function called "joinArrayOfArrays". Given an array of arrays, "joinArrayOfArrays" returns a single array containing the elements of the nested arrays. */

var output = joinArrayOfArrays([[1, 4], [true, false], ['x', 'y']]);

function joinArrayOfArrays(arr) {
  return arr.concat()
}

console.log(output); // --> [1, 4, true, false, 'x', 'y']



