/* Write a function called "removeFromBackOfNew". Given an array, "removeFromBackOfNew" returns a new array containing all but the last element of the given array. */

var arr = [1, 2, 3];
var output = removeFromBackOfNew(arr);

function removeFromBackOfNew(arr) {
  return arr.pop() // 3 & [1,2]
}

console.log(output); // --> [1, 2]
console.log(arr); // --> [1, 2, 3]




